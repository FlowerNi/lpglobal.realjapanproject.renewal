#mv .slider-box {
  height: 100vh;
  width: 100%;
  margin: 0;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  z-index: 1;
}

#mv .mv-slider,
#mv .mv-slider .slick-list,
#mv .mv-slider .slick-track,
#mv .mv-slider li {
  height: 100vh;
  width: 100%;
}

#mv .slider-box li {
  margin: 0;
  height: 100vh;
  width: 100%;
  overflow: hidden;
}

#mv .slider-box img {
  display: block;
  width: 100%;
  min-width: 100vw;
  height: 100vh !important;
  object-fit: cover;
  object-position: center;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}

/* Maintain slick dots */
/* Override the common.css slick dots specifically for #mv */
#mv .slick-dots {
  position: absolute;
  bottom: 37px;
  list-style: none;
  display: flex !important;
  text-align: center;
  padding: 0;
  margin: 0;
  width: 100%;
  justify-content: center;
  gap: 16px;
  z-index: 10;
}

#mv .slick-dots li {
  position: relative;
  display: inline-block;
  width: 80px;
  height: 4px;
  margin: 0;
  padding: 0;
  cursor: pointer;
}

#mv .slick-dots li button {
  font-size: 0;
  line-height: 0;
  display: block;
  width: 100%;
  height: 100%;
  cursor: pointer;
  color: transparent;
  border: 0;
  outline: none;
  background: transparent;
  padding: 0;
}

#mv .slick-dots li button:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 0.5);
  -webkit-font-smoothing: antialiased;
  content: '•';
  text-align: center;
  color: #fff;
  opacity: 0.5;
  -webkit-font-smoothing: antialiased;
}

#mv .slick-dots li button:hover:before {
  background: rgba(255, 255, 255, 0.8);
}

#mv .slick-dots li.slick-active button:before {
  background: #fff;
}

@media screen and (max-width: 768px) {
  #mv .slider-box {
    order: -1;
  }

  #mv .slider-box img {
    height: 100vh !important;
  }
}

