/*/////////////////////////////////////////
//  Header
/////////////////////////////////////////*/
#header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1000;
  background: transparent;
  padding: 20px 40px;
  transition: background 0.3s ease;
}

.header-inner {
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  max-width: 1580px;
}

.header-inner .logo {
  margin: 0;
}

.header-inner .logo img {
  width: 80px;
  height: auto;
  display: block;
}

.contact-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #d84a35;
  color: #fff;
  padding: 8px 24px;
  font-size: 14px;
  font-weight: 700;
  text-decoration: none;
  transition: background-color 0.3s ease;
}

.contact-icon:hover {
  background-color: rgb(224, 112, 96);
  text-decoration: none;
}

/*/////////////////////////////////////////
//  MV
/////////////////////////////////////////*/
#mv {
  width: 100%;
  height: 100vh;
  position: relative;
  overflow: hidden;
  left: 0;
  right: 0;
}

/* Add dark overlay for entire first view */
#mv::before {
  content: '';
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 100vw;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 2;
}

#mv .inner {
  height: 100%;
  width: 100vw;
  position: relative;
  z-index: 3;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
  max-width: none;
}

@media screen and (max-width: 768px) {
  #mv .inner {
    flex-direction: column;
    gap: 24px;
    padding-left: 32px;
    padding-right: 32px;
    align-items: center;
    padding-top: 24px;
  }

  /* Remove the third gap: space between hero image (slider) and the first paragraph */
  #mv .slider-box {
    margin-bottom: -24px;
  }
}

/* Hero mobile refinements */
@media screen and (max-width: 768px) {

  /* Hide mobile line-breaks inside headline */
  #mv .contents-area h1 br.sp {
    display: none;
  }

  /* Match txt2 font-size to h1 on tablet/mobile */
  #mv .contents-area .txt2 {
    font-size: 20px;
    /* align with h1 at <=768px */
    margin-bottom: 0;
    /* remove gap before button */
  }

  /* Remove extra paddings and tighten gaps after the first block */
  #mv .contents-area .txt1 {
    padding: 0;
  }

  #mv .contents-area .txt2 {
    padding: 0;
    margin-top: 12px;
  }

  /* Full-width, user-friendly button */
  #mv .contents-area .btn {
    max-width: none;
    width: 100%;
    margin: 0;
    /* remove extra margins */
    height: 64px;
    font-size: 18px;
    /* 18px */
  }
}

@media screen and (max-width: 480px) {
  #mv .inner {
    padding-left: 12px;
    padding-right: 12px;
    gap: 24px;
  }

  /* Keep the same adjustment for small mobile */
  #mv .slider-box {
    margin-bottom: -24px;
  }
}

@media screen and (max-width: 480px) {

  /* Keep txt2 matching h1 size on small mobile */
  #mv .contents-area .txt2 {
    font-size: 18px;
    /* align with h1 at <=480px */
  }

  /* Remove paddings also on small mobile */
  #mv .contents-area .txt1,
  #mv .contents-area .txt2 {
    padding: 0;
  }

  /* Button remains full-width with comfortable height */
  #mv .contents-area .btn {
    height: 60px;
  }
}

#mv .contents-area {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 3;
  width: 100%;
  display: flex;
